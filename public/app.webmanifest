    {
      "src": "/icons/favicon/favicon-32x32.png",
      "type": "image/png",
      "sizes": "32x32"
    },
    {
      "src": "/icons/favicon/favicon-192x192.png",
      "type": "image/png",
      "sizes": "192x192"
    },
    {
      "src": "/icons/favicon/favicon-512x512.png",
      "type": "image/png",
      "sizes": "512x512"
