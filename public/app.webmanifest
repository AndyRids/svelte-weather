  "description": "PWA weather app, using OpenWeatherMap API",
  "start_url": "/index.html",
  "scope": "/",
  "display": "fullscreen",
  "orientation": "portrait",
  "icons": [
    {
      "src": "/icons/favicon/favicon-16x16.png",
